@model PrimoCeramic.Models.Emails
@{
    ViewData["Title"] = "Delete";
}

<br />
<br />
<br />

<h2 class="text-info">Delete Product Type</h2>

<form method="post" asp-action="Delete">
    <div class="p-4 border rounded">


        <div class="form-group row">
            <div class="col-2">

                <label asp-for="Email"></label>
            </div>
            <div class="col-5">

                <input asp-for="Email" disabled class="form-control" />
            </div>

        </div>
        <div class="form-group row">
            <div class="col-2">

                <label asp-for="EmailType"></label>
            </div>
            <div class="col-5">

                <input asp-for="EmailType" disabled class="form-control" />
            </div>

        </div>
        <div class="form-group row">
            <div class="col-2">

                <label asp-for="Port"></label>
            </div>
            <div class="col-5">

                <input asp-for="Port" disabled class="form-control" />
            </div>

        </div>
        <div class="form-group row">
            <div class="col-2">

                <label asp-for="Map"></label>
            </div>
            <div class="col-5">

                <input asp-for="Map" disabled class="form-control" />
            </div>

        </div>
     

        <br />
        <div class="form-group">
            <input type="submit" class="btn btn-danger" asp-route-id="@Model.Id" value="Delete" />
            <a asp-action="Index" class="btn btn-primary">Back to List</a>
        </div>
    </div>
</form>

